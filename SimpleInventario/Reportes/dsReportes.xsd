                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT
    EntradasId as Folio,
    Productos.Nombre,
    Unidades.Unidad,
    Entradas.Precio,
    Entradas.Cantidad,
    Entradas.TotalImporte,
    'Entrada' as Tipo,
    Entradas.Fecha
FROM Productos
    INNER JOIN Entradas on Entradas.FKProductosId=ProductosId
    INNER JOIN Unidades on FkUnidadesId=UnidadesId
UNION
SELECT
    SalidasId as Folio,
    Productos.Nombre,
    Unidades.Unidad,
    Salidas.Precio,
    Salidas.Cantidad*-1,
    Salidas.TotalImporte,
    'Salida' as Tipo,
    Salidas.Fecha
FROM Productos
    INNER JOIN Salidas on Salidas.FKProductosId=ProductosId
    INNER JOIN Unidades on FkUnidadesId=UnidadesId
ORDER by Nombre, Fecha</CommandText>
